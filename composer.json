{
  "name": "log1x/gutentweaks",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "A plugin containing minor Gutenberg tweaks.",
  "keywords": [
    "wordpress"
  ],
  "autoload": {
    "psr-4": {
      "Log1x\\GutenTweaks\\": "src/"
    }
  },
  "require": {
    "php": ">=7.2.5",
    "matthiasmullie/minify": "^1.3"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5"
  },
  "scripts": {
    "lint": [
      "phpcs --ignore=public,vendor --extensions=php --standard=PSR12 ."
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
